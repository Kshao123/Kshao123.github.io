<h2>📦 Install</h2>
<pre><code class="language-bash">npm i course-table
npm i course-table -S</code></pre>
<h2>🔨 Usage</h2>
<pre><code class="language-jsx">import CourseTable from 'course-table';

const courseTables = {
      1: [
        {
          startTime:1551920827000,
          endTime:1551924427000,
          stuNameList: ['123'],
          teaName: '312'
        }
      ]
    };

const handleConfirm = (data,handleOK) =&gt; {
    handleOK()
};

ReactDOM.render(
    &lt;CourseTable 
        courseTables={courseTables}
        handleConfirm={handleConfirm}
    /&gt;, 
    mountNode);</code></pre>]]></content:encoded>
